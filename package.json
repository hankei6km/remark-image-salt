{
  "name": "remark-image-salt",
  "version": "0.1.0",
  "description": "remark plugin that generates HTML tags using attributes embedded in alt text of Image",
  "author": "hankei6km <hankei6km@gmail.com> (https://github.com/hankei6km)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/hankei6km/remark-image-salt.git"
  },
  "bugs": {
    "url": "https://github.com/hankei6km/remark-image-salt/issues"
  },
  "keywords": [
    "remark",
    "image"
  ],
  "main": "dist/index.js",
  "exports": "./dist/index.js",
  "type": "module",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "bin": {
    "image-salt": "dist/main.js"
  },
  "scripts": {
    "start": "npm run build && node dist/main.js",
    "build": "npm run clean && tsc && rimraf dist/test && mv dist/src/* dist/ && rimraf dist/src",
    "test": "node --experimental-vm-modules node_modules/.bin/jest",
    "clean": "rimraf \"dist/*\"",
    "upgrade-interactive": "npm-check --update",
    "csb:test": "npm test -- --runInBand --watchAll"
  },
  "dependencies": {
    "hast-util-from-parse5": "^7.1.0",
    "hast-util-to-html": "^8.0.2",
    "parse5": "^6.0.1",
    "remark": "^14.0.1",
    "unified": "^10.1.0",
    "unist-util-visit-parents": "^5.1.0",
    "yargs": "^17.2.1"
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.1",
    "@types/unist": "^2.0.6",
    "@types/yargs": "^17.0.4",
    "jest": "^27.3.0",
    "nodemon": "^2.0.13",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.3.0",
    "typescript": "^4.4.4"
  }
}
